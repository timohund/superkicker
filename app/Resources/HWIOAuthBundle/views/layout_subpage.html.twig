{% extends "subpage.html.twig" %}
{% block content %}
	<div id="content-inner">
		{% for type, messages in app.session.flashBag.all %}
			{% for message in messages %}
				<div class="{{ type }}">
					{{ message|trans({}, 'FOSUserBundle') }}
				</div>
			{% endfor %}
		{% endfor %}
		<div>
			{% block hwi_oauth_content %}
			{% endblock hwi_oauth_content %}
		</div>
	</div>
{% endblock %}